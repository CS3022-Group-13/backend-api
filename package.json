{
  "name": "eims-web-api",
  "version": "0.0.1",
  "description": "Backend API for EIMS",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/main.js",
    "dev": "ts-node src/main.ts",
    "clean": "rm -dfr ./dist",
    "serve": "ts-node-dev --respawn --transpile-only src/main.ts",
    "migrate": "knex migrate:latest && knex seed:run"
  },
  "author": "Thilina Lakshan",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.13",
    "objection": "^2.2.6",
    "pg": "^8.5.1",
    "uuid": "^8.3.2",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/knex": "^0.16.1",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
